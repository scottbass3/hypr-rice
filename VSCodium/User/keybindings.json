// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+up",         // whatever keybinding you want
        "command": "cursorMove",
        "args": {
            "to": "up",
            "by": "line",
            "value": 5         // change this if you want
        },
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+down",        // whatever keybinding you want
        "command": "cursorMove",
        "args": {
            "to": "down",
            "by": "line",
            "value": 5         // change
        },
        "when": "editorTextFocus"
    }
]